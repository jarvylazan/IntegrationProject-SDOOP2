<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="513.0" prefWidth="450.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.integrationprojectsdoop2.Controllers.SignUpController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="408.0" prefWidth="410.0" style="-fx-border-color: grey;">
      <children>
         <VBox prefHeight="15.0" prefWidth="408.0" />
         <Label alignment="CENTER" prefHeight="41.0" prefWidth="408.0" text="TJS MOVIE THEATER">
            <font>
               <Font name="System Bold" size="31.0" />
            </font>
         </Label>
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="444.0" text="Sign Up">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <VBox prefHeight="11.0" prefWidth="408.0" />
         <TextField fx:id="SignUpName" prefHeight="36.0" prefWidth="386.0" promptText="John Doe">
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </TextField>
         <VBox prefHeight="15.0" prefWidth="408.0" />
         <TextField fx:id="SignUpEmail" prefHeight="36.0" prefWidth="410.0" promptText="email">
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </TextField>
         <VBox prefHeight="15.0" prefWidth="408.0" />
         <PasswordField fx:id="SignUpPassword" prefHeight="36.0" prefWidth="410.0" promptText="Password">
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </PasswordField>
         <VBox prefHeight="15.0" prefWidth="408.0" />
         <PasswordField fx:id="SignUpConfirmPassword" prefHeight="36.0" prefWidth="410.0" promptText="Retype Password">
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </PasswordField>
         <VBox prefHeight="23.0" prefWidth="408.0" />
          <Button defaultButton="true" onAction="#onSighOnClickButton" prefHeight="36.0" prefWidth="388.0" style="-fx-background-color: #82d669; -fx-text-fill: white;" text="Sign Up">
            <font>
               <Font name="System Bold" size="13.0" />
            </font>
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </Button>
         <VBox prefHeight="8.0" prefWidth="408.0" />
         <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancelClickButton" prefHeight="36.0" prefWidth="388.0" text="Cancel">
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </Button>
      </children>
   </VBox>
   <VBox prefHeight="8.0" prefWidth="410.0" />
</VBox>
